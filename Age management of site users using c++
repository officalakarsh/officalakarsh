#include <iostream>

int main() {
    int age;
    std::cout<<"Enter your age?";
    std::cin>> age;
    
    if (age >=100){
        std::cout<<"Tau ganndi baat nhi :(";
    }
    else if(age>=18){
        std::cout<<"Your are welcome to our site!";
    }
    else if (age < 0){
        std::cout<<"Paida toh hoja pahle";
    }
   
    else{
    std::cout<<"YOU ARE NOT OLD ENOUGH!!!";
    }
    return 0;
}
